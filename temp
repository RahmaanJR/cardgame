get color(){
    return this.suit === "♣" || this.suit === "♠" ? "black" :  "red"
}

getHTML(){
    const cardDiv = document.createElement('div')
    cardDiv.innerText = this.suit
    cardDiv.classList.add("card", this.color)
    cardDiv.dataset.value = `${this.value} ${this.suit}`
    return cardDiv
}